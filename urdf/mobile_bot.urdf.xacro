<?xml version="1.0"?>
<robot name="messi_bot" xmlns:xacro="http://www.ros.org/wiki/xacro">

<!--Body dimentions-->
<xacro:property name="body_link_x_dim" value="1.0"/>
<xacro:property name="body_link_y_dim" value="0.6"/>
<xacro:property name="body_link_z_dim" value="0.3"/>
<!--wheel dimentions -->
<xacro:property name="wheel_link_radius" value="0.15"/>
<xacro:property name="wheel_link_length" value="0.1"/>
<xacro:property name="wheel_link_z_location" value="-0.1"/>


<!--material density xacro property-->
<xacro:property name="body_density" value="2710.0"/>
<!--Pi constant-->
<xacro:property name="pi_constant" value="3.14159265"/>
<!--body mass property-->
<xacro:property name="body_mass" value="${body_density*body_link_x_dim*body_link_y_dim*body_link_z_dim}"/>
<xacro:property name="cylindrical_mass"  value="${body_density*pi_constant*wheel_link_radius*wheel_link_radius*wheel_link_length}"/>
<!--Moments of inertia of wheel with respect to z-->
<xacro:property name="Iz_wheel" value="${0.5*cylindrical_mass*wheel_link_radius*wheel_link_radius}"/>
<!--Moments of inertia of wheel with respect to x-->
<xacro:property name="I_wheel" value="${(1.0/12.0)*cylindrical_mass*(3.0*wheel_link_radius*wheel_link_radius+wheel_link_length*wheel_link_length)}"/>

<!--implement complete inertia into interia section-->
<xacro:macro name="intertia_wheel">
<inertial>
<origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    <mass value="0.0"/>
    <inertia ixx="{I_wheel}" ixy="0.0" ixz="0.0" iyy="${I_wheel}" iyz="0" izz="${Iz_wheel}"/>

</inertial>
</xacro:macro>

<!--define sudo link-->
<link name="baseframe_link"/>

    <!-- Base Link -->
    <link name="base_link">
        <visual>
            <geometry>
                <box size="${body_link_x_dim} ${body_link_y_dim} ${body_link_z_dim}"/> <!-- Halved size -->
            </geometry>
            <material name="red">
                <color rgba="1 0 0"/>
            </material>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            
        </visual>
        <collision>
            <geometry>  <!-- Gripper control plugin specifics here, if needed -->
                <box size="${body_link_x_dim} ${body_link_y_dim} ${body_link_z_dim}"/> <!-- Halved size -->
            </geometry>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            
        </collision>
        <inertial>
            <mass value="${body_mass}"/>
            <inertia ixx="${(1/12)*body_mass*(body_link_y_dim*body_link_y_dim+body_link_z_dim*body_link_z_dim)}" ixy="0" 
            ixz="0" iyy="${(1/12)*body_mass*(body_link_x_dim*body_link_x_dim+body_link_z_dim*body_link_z_dim)}" iyz="0.0" 
            izz="${(1/12)*body_mass*(body_link_x_dim*body_link_x_dim+body_link_y_dim*body_link_y_dim)}"/><!-- Adjusted Inertia -->
        </inertial>
    </link>
<!--define sudo joint for gazebo-->
<joint name="baseframe_joint" type="fixed">
<parent link="baseframe_link"/>
<child link="base_link"/>

</joint>
