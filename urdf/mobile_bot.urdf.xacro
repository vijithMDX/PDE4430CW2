</gazebo>
<gazebo reference="gripper">
    <material>Gazebo/Grey</material>
</gazebo>

<!-- Plugins for wheels, laser scanner, and other functionalities -->
<gazebo>
    <plugin name="messi_bot_controller" filename="libgazebo_ros_diff_drive.so">
    <update_rate>100.0</update_rate>
    <robotNamespace></robotNamespace>
        <leftRearJoint>left_wheel_joint</leftRearJoint>
        <rightRearJoint>right_wheel_joint</rightRearJoint>
        <legacyMode>false</legacyMode>
        <robotBaseFrame>base_link</robotBaseFrame>

        <wheelSeparation>${body_link_y_dim+wheel_link_length}</wheelSeparation> <!-- Adjusted for reduced size -->
        <wheelDiameter>${wheel_link_radius*2}</wheelDiameter> <!-- Adjusted for reduced size -->
        <publishWheelJointState>true</publishWheelJointState>
        <torque>1000</torque>
        <commandTopic>cmd_vel</commandTopic>
        <odometryTopic>odom</odometryTopic>
        <odometryFrame>odom</odometryFrame>
        <publishWheelTF>true</publishWheelTF>
        <publishOdom>true</publishOdom>
        <covariance_x>0.0001</covariance_x>
        <covariance_y>0.0001</covariance_y>
        <covariance_z>0.0001</covariance_z>
    </plugin>
</gazebo>
<gazebo>
    <plugin name="joint_state_publisher" filename="libgazebo_ros_joint_state_publisher.so">
        <jointName>front_castor_wheel_joint</jointName>
    </plugin>
</gazebo>
<gazebo>
    <plugin name="gripper_controller" filename="libgazebo_ros_control.so">
        <robotNamespace>/</robotNamespace>
        <controlPeriod>0.001</controlPeriod>
        <robotSimType>gazebo_ros_control/DefaultRobotHWSim</robotSimType>
        <joint name="left_finger_joint">
            <hardwareInterface>EffortJointInterface</hardwareInterface>
        </joint>
        <joint name="right_finger_joint">
            <hardwareInterface>EffortJointInterface</hardwareInterface>
        </joint>
    </plugin>
</gazebo>


</robot>