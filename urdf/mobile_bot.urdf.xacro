    <!-- Joint for Left Finger -->
   <joint name="right_finger_joint" type="prismatic">
    <parent link="base_link"/>
    <child link="right_finger"/>
    <origin xyz="0.22 -0.13 0.01" rpy="0.0 0.0 0.0"/>
    <axis xyz="0 1 0"/> <!-- Axis along which the joint extends -->
    <limit lower="0.0" upper="0.05" effort="10" velocity="0.05"/>
</joint>


    <!-- Joint for Right Finger -->
  <joint name="left_finger_joint" type="prismatic">
    <parent link="base_link"/>
    <child link="left_finger"/>
    <origin xyz="0.22 0.13 0.01" rpy="0.0 0.0 0.0"/>
    <axis xyz="0 -1 0"/> <!-- Axis along which the joint extends -->
    <limit lower="0.0" upper="0.05" effort="10" velocity="0.05"/>
</joint>





<!-- Laser Scanner remains unchanged -->
 <link name="laser_scanner">
        <visual>
            <geometry>
                <box size="0.05 0.05 0.05"/>
            </geometry>
        </visual>

        <collision>
            <geometry>
                <box size="0.05 0.05 0.05"/>
            </geometry>
        </collision>

        <inertial>
            <mass value="1e-5"/>
            <inertia ixx="1e-6" ixy="0" ixz="0.0" iyy="1e-6" iyz="0.0" izz="1e-6"/>
        </inertial>
    </link>

    <joint name="laser_scanner_joint" type="fixed">
        <axis xyz="0.0 1 0.0"/>
        <parent link="base_link"/>
        <child link="laser_scanner"/>
        <origin xyz="0.0 0.0 0.08" rpy="0.0 0.0 0.0"/>
    </joint>
<!--Gazebo param-->
     <gazebo reference="laser_scanner">
        <sensor type="ray" name="laser">
            <pose>0 0 0 0 0 0</pose>
            <visualize>true</visualize>
            <update_rate>40</update_rate>
            <ray>
                <scan>
                    <horizontal>
                        <samples>720</samples>
                        <resolution>1</resolution>
                        <min_angle>-3.14</min_angle>
                        <max_angle>3.14</max_angle>
                    </horizontal>
                </scan>
                <range>
                    <min>0.1</min>
                    <max>10</max>
                    <resolution>0.1</resolution>
                </range>
            </ray>
            <plugin name="Lidar" filename="libgazebo_ros_laser.so">
                <topicName>/scan</topicName>
                <frameName>laser_scanner</frameName>
            </plugin>
        </sensor>
    </gazebo>

<!-- Gazebo plugins and materials -->
<gazebo reference="base_link">
<mu1>0.2</mu1>
<mu2>0.2</mu2>

    <material>Gazebo/Orange</material>
</gazebo>

<gazebo reference="front_castor_wheel_link">
<mu1>0.2</mu1>
<mu2>0.2</mu2>
    <material>Gazebo/Black</material>
</gazebo>
<gazebo reference="right_wheel_link">
<mu1>0.2</mu1>
<mu2>0.2</mu2>
    <material>Gazebo/Black</material>
</gazebo>
<gazebo reference="left_wheel_link">
<mu1>0.2</mu1>
